{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "img/**", "use": "@vercel/static" },
    { "src": "firstPage.css", "use": "@vercel/static" },
    { "src": "firstPage.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}